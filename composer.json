{
    "name": "profscode/translatable",
    "description": "File-based translatable package with versioning for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Profscode\\Translatable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Profscode\\Translatable\\TranslatableServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Yaman Hafez",
            "email": "yaman@profscode.com"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3 || ^8.4",
        "laravel/framework": "^11.0 || ^12.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}